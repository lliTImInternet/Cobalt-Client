                }
              },
              {
                "5@Cobalt_slider.Cobalt_slider_text": {
                  "$slider_name_controls": "player_list_opacity",
                  "$Cobalt_slider_text": "Background Opacity: §r0.3",
                  "$slider_value": 3
                }
              },
              {
                "6@Cobalt_slider.Cobalt_slider_text": {
                  "$slider_name_controls": "player_list_opacity",
                  "$Cobalt_slider_text": "Background Opacity: §r0.4",
                  "$slider_value": 4
                }
              },
              {
                "7@Cobalt_slider.Cobalt_slider_text": {
                  "$slider_name_controls": "player_list_opacity",
