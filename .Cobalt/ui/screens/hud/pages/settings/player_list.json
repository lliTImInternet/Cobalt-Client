              "100%c"
            ],
            "controls": [
              {
                "1@Cobalt_common.spacer": {}
              },
              {
                "2@Cobalt_slider.Cobalt_slider_text": {
                  "$slider_name_controls": "player_list_opacity",
                  "$Cobalt_slider_text": "Background Opacity: §r0.0",
