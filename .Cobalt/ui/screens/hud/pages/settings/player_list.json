{
    "namespace": "Cobalt_player_list_settings",
    "content@Cobalt_modules_menu_settings_pages.template": {
      "$Cobalt_settings_page_toggle_name": "Cobalt_player_list_settings",
      "$Cobalt_settings_page_header": "Player List Settings",
      "$Cobalt_settings_page_info": "The player list shows you the list of players currently on the server or world. You can activate it on PC by holding down the Tab key, or on mobile by tapping the P button.",
      "$Cobalt_settings_page_image": "player_list",
      "$Cobalt_settings_pages_toggles_controls": [
        {
            "gamer_icon_toggle@Cobalt_modules_menu_settings_pages.template_toggle": {
                "$Cobalt_settings_toggle_name": "Cobalt_player_list_gamer_icon_toggle",
